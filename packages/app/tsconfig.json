{
  "extends": "@youmeet/typescript-config/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "rootDirs": [
      "_components",
      "_homeComponents",
      "_sections",
      "[candidateName]",
      "backoffice",
      "competences",
      "compte",
      "conditions-generales-utilisation",
      "dashboard",
      "formulaire-profil",
      "le-produit",
      "medias",
      "mes-favoris",
      "offres",
      "profils"
    ],
    "outDir": "dist"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.test.tsx",
    "i18n/index.js",
    ".next/types/**/*.ts",
    "**/*.mjs",
    "next.config.js",
    "app/api/scripts/formatCompetencyTitle.mts",
    "app/api/scripts/addSubtitledUrl.ts",
    "app/api/scripts/formatCompetencyTitle.mts",
    "next.config.js",
    "CustomModal.tsx"
  ],
  "exclude": ["node_modules"]
}
