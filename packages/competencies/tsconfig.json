{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "allowJs": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "target": "ESNext",
    "declaration": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*.ts",
    "src/queries/*.graphql",
    "src/imports.js",
    "src/cjs/methods/getManyCompetencies.js",
    "src/cjs/methods/getOneCompetency.js",
    "src/cjs/index.js",
    "src/esm/index.js",
    "src/esm/methods/getManyCompetencies.js",
    "src/esm/methods/getOneCompetency.js"
  ],
  "exclude": ["node_modules"]
}
