{
  "name": "@youmeet/scripts",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "addSubtitledUrl": "SCRIPT=true dotenv -e .env.development -- ts-node ./addSubtitledUrl.ts",
    "createProfileViewsFor": "SCRIPT=true dotenv -e .env -- ts-node ./createProfileViewsFor.ts",
    "test": "vitest",
    "genCompetencies": "SCRIPT=true dotenv -e .env -- npx tsx ./genCompetencies.ts",
    "transferCompetencies": "SCRIPT=true dotenv -e .env -- npx tsx ./transferCompetencies.ts",
    "deleteCompetencies": "SCRIPT=true dotenv -e .env.development -- ts-node ./deleteCompetencies.ts",
    "testingAPIPackage": "SCRIPT=true dotenv -e .env.development -- npx tsx ./testingAPIPackage.ts",
    "importFTOffers": "SCRIPT=true dotenv -e .env.development -- npx tsx ./importFTOffers.ts",
    "checkDbIndexes": "SCRIPT=true dotenv -e .env.development -- ts-node ./checkDbIndexes.ts",
    "updateReducedOffers": "SCRIPT=true dotenv -e .env -- ts-node ./updateReducedOffers.ts",
    "renewDbProd": "SCRIPT=true dotenv -e .env -- npx tsx ./importFTOffers.ts && ts-node ./updateReducedOffers.ts && dotenv -e .env -- npx tsx ./collectOffersProd.ts && dotenv -e .env -- npx tsx ./connectOffersProd.ts",
    "renewDb": "SCRIPT=true dotenv -e .env.development -- npx tsx ./importFTOffers.ts && ts-node ./updateReducedOffers.ts",
    "removeBadIndexes": "SCRIPT=true dotenv -e .env -- npx tsx ./removeBadIndexes.ts",
    "collectOffers": "SCRIPT=true dotenv -e .env -- npx tsx ./collectOffers/script.ts",
    "connectOffers": "SCRIPT=true dotenv -e .env -- npx tsx ./connectOffers.ts",
    "collectAnConnect": "SCRIPT=true dotenv -e .env -- npx tsx ./collectOffers/script.ts && dotenv -e .env -- npx tsx ./connectOffers.ts",
    "sendEmailNotificationForNewOpportunities": "SCRIPT=true dotenv -e .env.development -- npx tsx ./sendEmailNotificationForNewOpportunities.ts",
    "sendEmailNotificationForNewOpportunitiesProd": "SCRIPT=true dotenv -e .env -- npx tsx ./sendEmailNotificationForNewOpportunities.ts",
    "sendEmailTheatre": "SCRIPT=true dotenv -e .env -- npx tsx ./sendEmailTheatre.ts",
    "tipe": "SCRIPT=true dotenv -e .env.development -- npx tsx ./tipe.ts",
    "tipe2": "SCRIPT=true dotenv -e .env.development -- npx tsx ./tipe2.ts",
    "welcomeEmail": "SCRIPT=true dotenv -e .env -- npx tsx ./welcomeEmail.ts",
    "fft": "SCRIPT=true dotenv -e .env.development -- npx tsx ./fft.ts",
    "applyForJob": "SCRIPT=true dotenv -e .env.development -- npx tsx ./applyForJob.ts",
    "test-collect": "NODE_ENV=test dotenv -e .env.development -- npx tsx ./collectOffers/script.ts",
    "test-connect": "NODE_ENV=test dotenv -e .env.development -- npx tsx ./connectOffers.ts",
    "test-collect-connect": "NODE_ENV=test dotenv -e .env.development -- npx tsx ./collectOffers/script.ts && NODE_ENV=test dotenv -e .env.development -- npx tsx ./connectOffers.ts"
  },
  "dependencies": {
    "@getbrevo/brevo": "^2.2.0",
    "@youmeet/competencies": "*",
    "@youmeet/functions": "*",
    "@youmeet/gql": "*",
    "@youmeet/models": "*",
    "@youmeet/performance": "*",
    "@youmeet/prisma-config": "*",
    "@youmeet/raw-data": "*",
    "@youmeet/utils": "*",
    "cloudinary": "^2.5.1",
    "dotenv": "^16.4.5",
    "fft.js": "^4.0.4",
    "mongodb": "^6.13.0",
    "mongoose": "^8.8.0",
    "openai": "^4.70.3",
    "pidusage": "^4.0.1",
    "puppeteer": "^24.3.0",
    "reflect-metadata": "^0.2.2",
    "systeminformation": "^5.27.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "babel-jest": "^29.7.0",
    "cypress": "^14.4.1",
    "jest": "^29.7.0",
    "vitest": "^3.0.7"
  }
}
