  "scripts": {
    "dev": "PORT=3001 dotenv -e .env.development -- next dev",
    "build": "next build",
    "analyse": "dotenv -e .env.development -- concurrently \"npx tailwindcss -c ./tailwind.config.ts \" \"next build\"",
