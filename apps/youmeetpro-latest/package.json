    "isValidObjectId": "SCRIPT=true dotenv -e .env -- ts-node ./app/api/scripts/isValidObjectId.ts",
    "format": "yarn prisma format",
    "db-push-dev": "dotenv -e .env.development -- yarn prisma db push",
    "db-push": "dotenv -e .env -- yarn prisma db push",
    "generate": "yarn prisma generate"
